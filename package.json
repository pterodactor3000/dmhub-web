{
  "name": "web-hub",
  "version": "0.1.0",
  "scripts": {
    "set:env": "ts-node ./src/environments/set-env.ts",
    "ng": "ng",
    "start": "npm run set:env && ng serve",
    "start:server": "http-server -p 4550 -c-1 -P http://localhost:4550? dist/web-hub",
    "build": "npm run set:env && ng build",
    "build:dev": "npm run set:env && ng build --configuration development",
    "build:watch": "npm run set:env && ng build --watch --configuration production",
    "watch": "npm run set:env && ng build --watch --configuration development",
    "test": "npm run set:env && ng test",
    "lint": "npm run set:env && ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.7",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.2.7",
    "@angular/compiler": "^14.2.7",
    "@angular/core": "^14.2.7",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^14.2.7",
    "@angular/localize": "^14.2.7",
    "@angular/platform-browser": "^14.2.7",
    "@angular/platform-browser-dynamic": "^14.2.7",
    "@angular/router": "^14.2.7",
    "@angular/service-worker": "^14.2.7",
    "@ng-bootstrap/ng-bootstrap": "^13.1.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "dotenv": "^16.0.3",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.2",
