    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.7",
    "@types/jasmine": "~4.3.0",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.1",
